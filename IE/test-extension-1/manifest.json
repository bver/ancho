{
  "name": "Seiberspace1",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://www.seiberspace.de/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.seiberspace.de/*"],
      "js": ["content.js"]
    }
  ]
}